<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ejerciciosdiplomado.ejercicio_two.ui.frame.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_frame" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.ejerciciosdiplomado.ejercicio_two.ui.linear.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_linear" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.ejerciciosdiplomado.ejercicio_two.ui.relative.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_relative" />

    <fragment
        android:id="@+id/navigation_constraint"
        android:name="com.example.ejerciciosdiplomado.ejercicio_two.ui.constraint.ConstraintFragment"
        android:label="@string/title_contraint"
        tools:layout="@layout/fragment_constraint" />
</navigation>